# Git
alias g="git"
alias gl="git l"
alias gA="git add -A; git s"
alias gr="grh"
alias gR="git reset -q; git s"
alias gs="git s"
alias gc="git commit"

# Clear shortcut
alias c="clear && tmux clear-history"

# Wifi Management
alias wifi="nmcli d wifi list"

# Configuration
alias vimrc="vim ~/.config/nvim/init.vim"
alias tmuxrc="vim ~/.tmux.conf && tmux source-file ~/.tmux.conf"
